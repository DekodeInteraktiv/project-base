{
	"$schema": "https://schemas.wp.org/trunk/wp-env.json",
	"core": "./public/wp",
	"phpVersion": "8.4",
	"plugins": [],
	"themes": [],
	"config": {
		"AUTOMATIC_UPDATER_DISABLED": true
	},
	"mappings": {
		"../packages": "./packages",
		"../vendor": "./vendor",
		"tools": "./tools",
		"wp-content": "./public/content"
	},
	"lifecycleScripts": {
		"afterStart": "rm ./wp-content/object-cache.php & npm run wp-env -- run cli ./tools/local/setup-main-site.sh"
	}
}
